{
  "description": "MobileVLM single layer (MobileVLM1.7B (MobileLLama1.4B) 24 layers, MobileVLM3B (MobileLLama2.7B) 32 layers) (Attn&weights @DRAM, FFN&weights @RRAM)",
  "tensors": [
    {"name": "tokens_pe",    "shape": [2048, 2560], "bits": 16, "device": "dram"},

    {"name": "W_q_0", "shape": [2560, 160], "bits": 4, "device": "dram"},
    {"name": "W_k_0", "shape": [2560, 160], "bits": 4, "device": "dram"},
    {"name": "W_v_0", "shape": [2560, 160], "bits": 4, "device": "dram"},
    {"name": "W_o_0", "shape": [2560, 2560], "bits": 4, "device": "dram"},
    {"name": "W1_0",  "shape": [2560, 20480], "bits": 4, "device": "rram"},
    {"name": "W2_0",  "shape": [2560, 20480], "bits": 4, "device": "rram"},

    {"name": "norm1_out",    "shape": [2048, 2560], "bits": 16, "device": "dram"},
    {"name": "Q",            "shape": [2048, 2560], "bits": 16, "device":  "dram"},
    {"name": "K",            "shape": [2048, 2560], "bits": 16, "device":  "dram"},
    {"name": "V",            "shape": [2048, 2560], "bits": 16, "device":  "dram"},
    {"name": "attn_scores",  "shape": [2048, 2048], "bits": 16, "device":  "dram"},
    {"name": "attn_probs",   "shape": [2048, 2048], "bits": 16, "device":  "dram"},
    {"name": "attn_ctx",     "shape": [2048, 2560], "bits": 16, "device":  "dram"},
    {"name": "attn_out",     "shape": [2048, 2560], "bits": 16, "device":  "rram"},
    {"name": "attn_out_r",   "shape": [2048, 2560], "bits": 16, "device":  "rram"},
    {"name": "resid1",       "shape": [2048, 2560], "bits": 16, "device":  "dram"},
    {"name": "norm2_out",    "shape": [2048, 2560], "bits": 16, "device":  "rram"},
    {"name": "ff2",          "shape": [2048, 2560], "bits": 16, "device":  "rram"}
  ],
  "ops": [

    {"type": "LayerNorm", "A": "tokens_pe", "C": "norm1_out"} ,
    {"type": "ParallelOps", "branches": [
      {"type": "MatMul", "A": "norm1_out", "B": "W_q_0", "C": "Q"},
      {"type": "MatMul", "A": "norm1_out", "B": "W_k_0", "C": "K"},
      {"type": "MatMul", "A": "norm1_out", "B": "W_v_0", "C": "V"}
    ]},
    {"type": "MatMul", "A": "Q", "B": "K", "C": "attn_scores", "transpose_B": true},
    {"type": "SoftmaxOp", "input": "attn_scores", "axis": -1, "output": "attn_probs"},
    {"type": "MatMul", "A": "attn_probs", "B": "V", "C": "attn_ctx"},
    {"type": "MatMul", "A": "attn_ctx", "B": "W_o_0", "C": "attn_out"},
    {"type": "AddOp", "A": "attn_out", "B": "tokens_pe", "C": "resid1"},
    {"type": "LayerNorm", "A": "resid1", "C": "norm2_out"},
    {"type": "UCIeOp", "size_bits": 83886080},
    {"type": "MatMul", "A": "norm2_out", "B": "W1_0", "C": "attn_out_r"},
    {"type": "GeluOp", "A": "attn_out_r", "C": "attn_out_r"}, 
    {"type": "MatMul", "A": "attn_out_r", "B": "W2_0", "C": "ff2"},
    {"type": "UCIeOp", "size_bits": 83886080},
    {"type": "AddOp", "A": "ff2", "B": "resid1", "C": "tokens_pe"}

  ]
}